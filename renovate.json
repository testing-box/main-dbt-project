{
  "extends": [
    "config:base"
  ],
  "regexManagers": [    
    {
      "fileMatch": ["packages.yml"],
      "matchStrings": ["revision: (?<currentValue>.*?) # renovate: depName=(?<depName>.*)"],
      "datasourceTemplate": "github-releases"
    }
  ],
  "packageRules": [
    {
      "matchPackageNames": ["dockerfile"],
      "enabled": false
    },
    {
      "groupName": "dbt",
      "matchPackagePrefixes": ["testing-box/dbt"]
    } 
  ]
}
