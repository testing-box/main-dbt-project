{
  "extends": [
    "config:base"
  ],
  "regexManagers": [    
    {
      "fileMatch": ["packages.yml"],
      "matchStrings": [
        "revision: (?<currentValue>.*?) # renovate: depName=(?<depName>.*)",
        "version: (?<currentValue>.*?) # renovate: depName=(?<depName>.*)"
      ],
      "datasourceTemplate": "github-releases"
    },
    {
      "fileMatch": ["test.py"],
      "matchStrings": [
        "image=f"{config.AWS_ECR_URI}/tap-ukg-dimensions:(?<currentValue>.*?) # renovate: depName=(?<depName>.*)""
      ],
      "datasourceTemplate": "github-releases"
    }
  ],
  "packageRules": [
    {
      "groupName": "dbt",
      "matchPackagePrefixes": ["testing-box/dbt"],
      "separateMajorMinor": false
    } 
  ],  
  "enabledManagers": ["regex"]
}
